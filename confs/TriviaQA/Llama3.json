{
    "model_path": {
        "main_model_path": "meta-llama/Meta-Llama-3-8B-Instruct"
    },
    "file_path": {
        "dev_file_path": "./datasets/TriviaQA/wikipedia-dev-1900.jsonl",
        "test_file_path": "./datasets/TriviaQA/wikipedia-test-6000.jsonl",
        "demon_file_path": "./datasets/TriviaQA/wikipedia-demon-all_sample5_seed42.jsonl"
    },
    "prompt_template": {
        "main_model_system_template": "<s>{}",
        "assist_model1_system_template": "<s>{}",
        "instruction": "Answer this questions:\n",
        "instruction_parameter": {
            "key": [
                "question"
            ],
            "template": "Question:{}\nAnswer:"
        },
        "demon_parameter": {
            "key": [
                "question",
                "answer"
            ],
            "template": "Question:{}\nAnswer:{}\n"
        }
    },
    "run_parameter": {
        "max_new_tokens": 10
    },
    "result_process_parameter": {
        "early_stop_string_list": [
            "\nQuestion:"
        ],
        "split_key_before": [
            "\nAnswer:"
        ],
        "split_key_behind": [
            "\n",
            "</s>",
            "<unk>",
            "\nQuestion:"
        ]
    }
}